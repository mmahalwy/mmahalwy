{
  "name": "next-mdx-static-blog-kit",
  "version": "1.0.0",
  "description": "A starter kit for creating a static website blog using Next.js and MDX",
  "scripts": {
    "generate-list": "node ./utils/generatePostList.js",
    "dev": "npm run generate-list; NODE_ENV=development next",
    "build": "npm run generate-list; NODE_ENV=production next build",
    "build-dev": "npm run generate-list; NODE_ENV=development next build",
    "start": "NODE_ENV=production next start",
    "clean": "rm -rf .next ; npm ci ; NODE_ENV=development npm run dev",
    "test": "jest"
  },
  "keywords": [
    "next.js",
    "react.js",
    "blog",
    "mdx"
  ],
  "author": "Loren Stewart",
  "license": "ISC",
  "bin": {
    "blog": "cli.js"
  },
  "browserslist": [
    "last 2 version"
  ],
  "dependencies": {
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/loader": "^1.5.2",
    "@zeit/next-mdx": "^1.2.0",
    "babel-plugin-preval": "^4.0.0",
    "chalk": "^3.0.0",
    "js-yaml": "^3.13.1",
    "js-yaml-loader": "^1.2.2",
    "lodash": "^4.17.15",
    "next": "^9.3.5",
    "next-fonts": "^0.19.0",
    "next-offline": "^5.0.0",
    "next-seo": "^4.4.0",
    "polished": "^3.4.4",
    "react": "^16.12.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-icons": "^3.8.0",
    "react-syntax-highlighter": "^9.0.1",
    "rehype-highlight": "^3.1.0",
    "remark-emoji": "^2.0.2",
    "remark-images": "^1.0.0",
    "scrollprogress": "^3.0.2",
    "styled-bootstrap-grid": "^3.1.0",
    "styled-components": "^4.4.1",
    "styled-reboot": "^4.0.3",
    "styled-system": "^5.1.5",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.17",
    "babel-plugin-styled-components": "^1.10.7",
    "file-loader": "^6.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.9.0",
    "next-optimized-images": "^2.5.4",
    "svg-sprite-loader": "^4.1.6",
    "typescript": "^3.7.4",
    "webp-loader": "^0.6.0"
  }
}
